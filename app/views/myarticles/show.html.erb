<h1>Show My Articles Details </h1>

<p>
  <strong>ID:</strong>
  <%= @article.id %>
</p>

<p>
  <strong>Title:</strong>
  <%= @article.title %>
</p>


<p>
  <strong>Description:</strong>
  <%= @article.description %>
</p>

<p>
  <strong>Created_at:</strong>
  <%= @article.created_at %>
</p>

<%= link_to 'Edit', edit_myarticle_path(@article) %> |
<%= link_to 'Delete', myarticle_path(@article), method: :delete, data: { confirm: "Are you sure?" } %> |
<%= link_to 'Return to articles listing', myarticles_path %> 

<p>
  <%= link_to 'Create new article', new_myarticle_path %>
</p>

